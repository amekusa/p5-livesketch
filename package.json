{
  "name": "p5-livesketch",
  "version": "1.0.1",
  "description": "A command line builder for p5.js sketches, which supports live preview and ES module loading",
  "main": "index.js",
  "files": [
    "scaffold.js",
    "themes/default",
    "sketches/example.js"
  ],
  "bin": {
    "p5": "index.js"
  },
  "scripts": {
    "postinstall": "cd node_modules/.bin && ln -sfn ../../index.js p5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amekusa/p5-livesketch.git"
  },
  "keywords": [
    "p5",
    "p5.js",
    "art",
    "design",
    "game dev",
    "cli",
    "sandbox",
    "playground"
  ],
  "author": "amekusa",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/amekusa/p5-livesketch/issues"
  },
  "homepage": "https://github.com/amekusa/p5-livesketch#readme",
  "dependencies": {
    "@rollup/plugin-url": "^6.1.0",
    "browser-sync": "^2.27.7",
    "cadept": "^2.1.0",
    "chalk": "^4.1.2",
    "del": "^6.0.0",
    "gulp": "^4.0.2",
    "gulp-if": "^3.0.0",
    "gulp-rename": "^2.0.0",
    "inquirer": "^8.2.0",
    "p5": "^1.4.0",
    "rollup": "^2.62.0",
    "yargs": "^17.3.1"
  },
  "bundleDependencies": [
    "p5"
  ]
}
